<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox fx:id="vbox" alignment="TOP_CENTER" focusTraversable="true" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onDragDetected="#onDragDetected" onDragDone="#onDragDone" onDragEntered="#onDragEntered" prefHeight="150.0" prefWidth="110.0" styleClass="normal" stylesheets="@../styles/imagecell.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.pdfproject.ImageCellController">
   <children>
      <HBox prefHeight="20.0" prefWidth="100.0">
         <children>
            <Label fx:id="name" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="20.0" prefWidth="80.0" text="Label" textAlignment="CENTER">
               <font>
                  <Font name="Segoe UI" size="12.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#DeleteCell" prefHeight="15.0" prefWidth="15.0" styleClass="delete" textAlignment="CENTER" translateX="2.5">
               <font>
                  <Font size="1.0" />
               </font>
               <graphic>
                  <ImageView fitHeight="15.0" fitWidth="15.0" pickOnBounds="true" preserveRatio="true" styleClass="delete">
                     <image>
                        <Image url="@../CloseButton.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
         </children>
      </HBox>
      <ImageView fx:id="image" fitHeight="120.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</VBox>
